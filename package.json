{
	"name": "lab.joellesenne.dev",
	"version": "1.0.1",
	"private": false,
	"description": "Various coding experiments",
	"keywords": [
		"gatsby"
	],
	"homepage": "https://lab.joellesenne.dev",
	"bugs": {
		"url": "https://github.com/joellesenne/lab.joellesenne.dev/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/joellesenne/lab.joellesenne.dev"
	},
	"license": "MIT",
	"author": "JoÃ«l Lesenne <contact@joellesenne.dev>",
	"scripts": {
		"build": "gatsby build",
		"develop": "gatsby develop -o",
		"clean": "rm -rf .cache & rm -rf public",
		"format": "prettier --write \"**/*.{js,jsx,json,md}\"",
		"lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
		"lint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
		"serve": "gatsby serve",
	  	"snyk-prepare": "npm run snyk-protect",
		"snyk-protect": "snyk-protect",
		"start": "npm run develop",
		"test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \""
	},
	"eslintConfig": {
		"env": {
			"browser": true,
			"node": true
		}
	},
	"dependencies": {
		"@snyk/protect": "1.981.0",
		"axios": "0.26.1",
		"gatsby": "4.17.0",
		"gatsby-image": "3.11.0",
		"gatsby-plugin-google-analytics": "4.17.0",
		"gatsby-plugin-manifest": "4.17.0",
		"gatsby-plugin-offline": "5.17.0",
		"gatsby-plugin-pnpm": "1.2.10",
		"gatsby-plugin-react-helmet": "5.17.0",
		"gatsby-source-filesystem": "4.17.0",
		"prop-types": "15.8.1",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-helmet": "6.1.0",
		"styled-components": "5.3.5"
	},
	"devDependencies": {
		"@babel/core": "7.17.5",
		"@babel/eslint-parser": "7.17.0",
		"dotenv": "16.0.0",
		"eslint": "8.11.0",
		"eslint-config-airbnb": "19.0.4",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-import": "2.25.4",
		"eslint-plugin-jsx-a11y": "6.5.1",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-react": "7.29.4",
		"eslint-plugin-react-hooks": "4.3.0",
		"gatsby-plugin-root-import": "2.0.8",
		"prettier": "2.6.0"
	},
	"engines": {
		"node": ">=16.0.0",
		"npm": ">=7.13.0"
	},
	"snyk": true,
	"pnpm": {
		"overrides": {
			"ansi-html@<0.0.8": ">=0.0.8",
			"node-fetch@<2.6.7": ">=2.6.7",
			"ansi-regex@>2.1.1 <5.0.1": ">=5.0.1",
			"immer@<9.0.6": ">=9.0.6",
			"ws@>=7.0.0 <7.4.6": ">=7.4.6",
			"browserslist@>=4.0.0 <4.16.5": ">=4.16.5"
		}
	}
}
